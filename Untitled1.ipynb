   "source": [
    "import pybamm\n",
    "\n",
    "model = pybamm.lithium_ion.SPM()\n",
    "\n",
    "for event in model.events:\n",
    "    print(event.name)\n",
    "    model.variables.update({event.name: event.expression})\n",
    "    \n",
    "sim = pybamm.Simulation(model)  \n",
    "sim.step(100)\n",
    "sim.solution[\"Maximum voltage\"].entries"
   ]
